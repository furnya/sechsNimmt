{
  "name": "sechsnimmt-backend",
  "description": "Sechsnimmt Backend Server",
  "version": "0.0.1",
  "private": true,
  "author": "Lino Steinhau",
  "engines": {
    "node": "14.x.x"
  },
  "scripts": {
    "prepare": "npm run gcp-build",
    "pretest": "npm run gcp-build",
    "test": "mocha test/*.test.js --exit",
    "posttest": "npm run lint",
    "lint": "tslint -p .",
    "start": "node ./index.js",
    "gcp-build": "tsc -p .",
    "deploy": "gcloud app deploy",
    "dev": "nodemon --config nodemon.json index.ts",
    "dev:debug": "nodemon --config nodemon.json --inspect-brk index.ts"
  },
  "dependencies": {
    "@types/express": "^4.16.0",
    "@types/node": "^14.14.22",
    "@types/socket.io": "^2.1.13",
    "@types/socket.io-client": "^1.4.35",
    "express": "^4.17.1",
    "firebase": "^8.2.5",
    "socket.io": "^3.1.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "ws": "^7.4.2"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/node": "^14.14.22",
    "@types/ws": "^7.4.0",
    "chai": "^4.2.0",
    "mocha": "^8.0.0",
    "nodemon": "^2.0.7",
    "tslint": "^6.0.0",
    "typescript": "^4.1.3",
    "wait-port": "^0.2.7"
  }
}
